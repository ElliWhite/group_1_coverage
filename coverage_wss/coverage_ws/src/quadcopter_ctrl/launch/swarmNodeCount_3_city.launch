<launch>
  <node name="OSListener" pkg="quadcopter_ctrl" type="OnlineSearchListener" args="3" output="screen"/>
<node name="quadNodeCount_0" pkg="quadcopter_ctrl" type="quadNodeCount" args="0 $(arg input) 1.0 sim 5 $(arg minVis)" output="screen"/>
<node name="quadNodeCount_1" pkg="quadcopter_ctrl" type="quadNodeCount" args="1 $(arg input) 2 sim 6 $(arg minVis)" output="screen"/>
<node name="quadNodeCount_2" pkg="quadcopter_ctrl" type="quadNodeCount" args="2 $(arg input) 3 sim 7 $(arg minVis)" output="screen"/>
